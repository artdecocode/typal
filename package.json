{
  "name": "typal",
  "version": "1.22.5",
  "description": "Organises TypeDefs By Placing Them Into Types.Xml File To Be Embedded Into Source Code Compatible With VSCode And Google Closure Compiler, Generates Externs And Allows To Place Documentation In README Markdown.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "dev": "alanode src/bin/typal",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "wiki": "DEPACK_MAX_COLUMNS=80 NODE_DEBUG=doc doc wiki -W ../typal.wiki -n _typal",
    "wiki-dev": "DEPACK_MAX_COLUMNS=80 NODE_DEBUG=doc doc-dev wiki -W ../typal.wiki -n _typal",
    "compile": "depack-dev src/bin/typal -c -o compile/bin -a -p -s --externs types/externs.js -O 2018",
    "compile-lib": "depack-dev src/depack -c -o compile/depack.js -a -p -s -O 2018 --source_map_include_content",
    "c": "yarn-s compile compile-lib",
    "template": "yarn dev compile/template.js -T compile/index.js -t types/API.xml",
    "depack1": "depack-dev depack/t.js -c -o t/tt.js -a -p",
    "depack2": "depack-dev depack/prop/index.js -c -a -p",
    "depack-compat": "depack-dev example/restream/compat -c -a -p",
    "compat": "yarn dev example/restream/compat.js -c",
    "d": "yarn-s d1 externs",
    "d1": "yarn dev types/index.js",
    "build": "yarn-s d b doc",
    "e": "alanode",
    "example/": "yarn e example/example.js",
    "splendid": "splendid-dev",
    "web": "splendid-dev -s",
    "typal": "depack/bin/typal.js",
    "externs": "yarn dev types/externs.js",
    "args": "argufy types/arguments.xml -o src/bin/get-args.js"
  },
  "bin": {
    "typal": "compile/bin/typal.js",
    "typal-dev": "src/bin/index.js"
  },
  "files": [
    "src",
    "compile",
    "types/externs.js",
    "types/index.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/typal.git"
  },
  "keywords": [
    "typal",
    "jsdoc",
    "typedef",
    "type",
    "types",
    "documentation",
    "doc",
    "gcc",
    "closure",
    "google closure compiler",
    "externs"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/typal/issues"
  },
  "homepage": "https://github.com/artdecocode/typal#readme",
  "devDependencies": {
    "@artdeco/clean-stack": "^1.1.1",
    "@typedefs/parser": "^1.3.4",
    "@wrote/read": "^1.0.4",
    "@wrote/read-dir-structure": "^1.2.0",
    "@wrote/write": "^1.0.4",
    "@zoroaster/mask": "^2.2.0",
    "alamode": "^3.0.0",
    "argufy": "^1.7.1",
    "catchment": "^3.3.0",
    "documentary": "^1.32.2",
    "erotic": "^2.1.1",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "makepromise": "^3.1.0",
    "mismatch": "^1.2.0",
    "preact": "8.5.2",
    "restream": "^3.7.4",
    "rexml": "^2.2.2",
    "temp-context": "^2.1.3",
    "usually": "^1.2.0",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.2"
  },
  "srcDependencies": {
    "@typedefs/parser": "^1.3.4",
    "@wrote/read": "^1.0.4",
    "mismatch": "^1.2.0",
    "rexml": "^2.2.2"
  }
}
